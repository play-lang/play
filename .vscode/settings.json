{
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.rulers": [100],
        "editor.wordWrapColumn": 100,
        "editor.formatOnSave": true
    },
    "cSpell.words": [
        "API's",
        "Aarne",
        "DECGLOBAL",
        "GETGLOBAL",
        "GREATEREQUAL",
        "GREATERTHAN",
        "GREATERTHANEQUAL",
        "INCGLOBAL",
        "JMPFALSE",
        "JMPFALSEPOP",
        "JMPT",
        "JMPTRUE",
        "JMPTRUEPOP",
        "JUMPFALSE",
        "JUMPFALSEPOP",
        "JUMPTRUE",
        "JUMPTRUEPOP",
        "LESSEQUAL",
        "LESSTHAN",
        "LESSTHANEQUAL",
        "Nystrom",
        "Parselets",
        "Ranta",
        "Ravi",
        "SETGLOBAL",
        "Sethi",
        "Thorsten",
        "Ullman",
        "backpatch",
        "bytecode",
        "josephessin",
        "parselet",
        "performant",
        "quotemark",
        "subtrees"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.tslint": true
    },
    "editor.rulers": [
        80
    ],
    "editor.wordWrapColumn": 80,
    "editor.wordWrap": "off",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.tsdk": "./node_modules/typescript/lib",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    }
}